@using Data.Model.APIWeb

<select class="form-control" id="sltCreateDistrictV">
    @{
        if ((List<ListDistrict>)ViewBag.ListDistrict != null)
        {
            foreach (var i in (List<ListDistrict>)ViewBag.ListDistrict)
            {
                <option value="@i.ID">@i.Name</option>
            }
        }
    }
</select>
